Function TraeCorrelativo() As Double
Dim AdoTemp As New ADODB.Recordset
Dim lstrSql As String

    TraeCorrelativo = 0

    lstrSql = "Select max(id_item) as Correlativo from Tllr_Campañas where Vin='" & Me.txtCodigo & "'"
    If Conexion.SendHost(lstrSql, AdoTemp, adOpenKeyset, adLockOptimistic, gcTiempoEspera) = apOk Then
        If Not AdoTemp.BOF And Not AdoTemp.EOF Then
            TraeCorrelativo = IIf(IsNull(AdoTemp!correlativo), 1, AdoTemp!correlativo + 1)
        End If
    End If
    Conexion.CloseHost AdoTemp

End Function

